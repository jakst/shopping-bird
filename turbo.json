{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"outputs": ["dist/**", ".cache/**"]
		},
		"app#build": {
			"outputs": ["dist/**", ".solid/**", ".output/**", ".vercel/output/**"],
			"env": ["VITE_*", "LOCAL_DEV"]
		},
		"start": {
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"test": {}
	}
}
