{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"outputs": ["dist/**"]
		},
		"app#build": {
			"outputs": ["dist/**", ".solid/**", ".output/**", ".vercel/output/**"],
			"dependsOn": ["^build"],
			"env": ["LOCAL_DEV"]
		},
		"start": {
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"test": {}
	}
}
