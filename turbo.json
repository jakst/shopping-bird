{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"outputs": ["dist/**", ".output"],
			"dependsOn": ["^build"],
			"env": ["LOCAL_DEV"]
		},
		"start": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"test": {}
	}
}
