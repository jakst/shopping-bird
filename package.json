{
	"name": "shopping-bird",
	"private": true,
	"packageManager": "pnpm@7.30.0",
	"scripts": {
		"app": "pnpm --filter app",
		"server": "pnpm --filter server",
		"lib": "pnpm --filter lib",
		"format": "prettier --cache --write .",
		"lint": "pnpm eslint -f unix .",
		"tc": "pnpm tsc -b",
		"check": "pnpm tc && pnpm lint",
		"clean": "git clean -xfd node_modules packages -e packages/app/.env -e packages/server/.env && pnpm install"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "^0.4.2",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"prettier": "^2.8.6",
		"turbo": "^1.8.5",
		"typescript": "^5.0.2"
	},
	"dependencies": {
		"@thisbeyond/solid-dnd": "^0.7.4"
	},
	"pnpm": {
		"patchedDependencies": {
			"solid-start@0.2.23": "patches/solid-start@0.2.23.patch"
		}
	}
}
